sf:grownIn a owl:ObjectProperty ;
    rdfs:domain sf:Crop ;
    rdfs:range sf:ClimateZone .

sf:hasWeatherCondition a owl:ObjectProperty ;
    rdfs:domain sf:Crop ;
    rdfs:range sf:WeatherCondition .

sf:receivesFertilizer a owl:ObjectProperty ;
    rdfs:domain sf:Crop ;
    rdfs:range sf:Fertilizer .

sf:requiresPractice a owl:ObjectProperty ;
    rdfs:domain sf:Crop ;
    rdfs:range sf:FarmingPractice .

sf:suitableForSoil a owl:ObjectProperty ;
    rdfs:domain sf:Crop ;
    rdfs:range sf:Soil .

sf:usesIrrigation a owl:ObjectProperty ;
    rdfs:domain sf:Crop ;
    rdfs:range sf:IrrigationMethod .

sf:Peaty a sf:Soil ;
    rdfs:label "Peaty" ;
    rdfs:comment "Rich in organic matter" .

sf:Surface a sf:IrrigationMethod ;
    rdfs:label "Surface" .

sf:DAP a sf:Fertilizer ;
    rdfs:label "DAP" .

sf:Dry a sf:WeatherCondition ;
    rdfs:label "Dry" .

sf:Flood a sf:IrrigationMethod ;
    rdfs:label "Flood" .

sf:Precision a sf:FarmingPractice ;
    rdfs:label "Precision" .

sf:Rainy a sf:WeatherCondition ;
    rdfs:label "Rainy" .

sf:Silty a sf:Soil ;
    rdfs:label "Silty" ;
    rdfs:comment "Smooth, retains moisture" .

sf:Arid a sf:ClimateZone ;
    rdfs:label "Arid" .

sf:Clay a sf:Soil ;
    rdfs:label "Clay" ;
    rdfs:comment "Heavy, retains water" .

sf:Hot a sf:WeatherCondition ;
    rdfs:label "Hot" .

sf:Sustainable a sf:FarmingPractice ;
    rdfs:label "Sustainable" .

sf:Compost a sf:Fertilizer ;
    rdfs:label "Compost" .

sf:Humid a sf:WeatherCondition ;
    rdfs:label "Humid" .

sf:Sandy a sf:Soil ;
    rdfs:label "Sandy" ;
    rdfs:comment "Light, drains quickly" .

sf:Urea a sf:Fertilizer ;
    rdfs:label "Urea" .

sf:ClimateZone a owl:Class .

sf:Cold a sf:ClimateZone,
        sf:WeatherCondition ;
    rdfs:label "Cold" .

sf:Conventional a sf:FarmingPractice ;
    rdfs:label "Conventional" .

sf:FarmingPractice a owl:Class .

sf:Fertilizer a owl:Class .

sf:IrrigationMethod a owl:Class .

sf:Loamy a sf:Soil ;
    rdfs:label "Loamy" ;
    rdfs:comment "Balanced, fertile" .

sf:NPK a sf:Fertilizer ;
    rdfs:label "NPK" .

sf:Organic a sf:FarmingPractice ;
    rdfs:label "Organic" .

sf:Sprinkler a sf:IrrigationMethod ;
    rdfs:label "Sprinkler" .

sf:Temperate a sf:ClimateZone ;
    rdfs:label "Temperate" .

sf:Soil a owl:Class .

sf:Tropical a sf:ClimateZone ;
    rdfs:label "Tropical" .

sf:WeatherCondition a owl:Class .

sf:Drip a sf:IrrigationMethod ;
    rdfs:label "Drip" .

sf:Crop a owl:Class .

